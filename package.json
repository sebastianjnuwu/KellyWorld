{
  "name": "kellyworld",
  "version": "3.0.4",
  "private": true,
  "description": "simple discord bot",
  "main": "build/main.js",
  "author": "sebastianjnuwu <sebastianjnuwu@gmail.com>",
  "license": "Apache-2.0",
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "build:watch": "tsc --project tsconfig.json -w",
