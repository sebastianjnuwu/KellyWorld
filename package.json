    "i18next": "^23.7.12",
    "i18next-fs-backend": "^2.3.1",
    "prisma": "^5.9.1",
    "undici": "^6.6.2",
    "util": "^0.12.5",
    "vulkava": "^1.6.2",
    "winston": "^3.8.1"
  },
  "devDependencies": {
    "@types/node": "^16.11.6",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sebastianjnuwu/KellyWorld"
  },
  "bugs": {
